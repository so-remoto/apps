    "concurrently": "^9.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
